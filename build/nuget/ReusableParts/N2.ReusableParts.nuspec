<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>N2CMS.ReusableParts</id>
		<version>{Version}</version>
		<title>N2CMS Reusable Parts</title>
		<authors>Benjamin Herila</authors>
		<owners>Benjamin Herila, Cristian Libardo</owners>
		<licenseUrl>http://www.boost.org/users/license.html</licenseUrl>
